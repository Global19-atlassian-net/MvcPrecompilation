<Project>
  <Import Project="..\version.props" />

  <PropertyGroup>
    <Product>Microsoft ASP.NET Core</Product>
    <RepositoryUrl>https://github.com/aspnet/MvcPrecompilation</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)../build/Key.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <PublicSign Condition="'$(OS)' != 'Windows_NT'">true</PublicSign>
    <Copyright Condition="'$(Copyright)'==''">Â© Microsoft Corporation. All rights reserved.</Copyright>
    <NeutralLanguage Condition="'$(NeutralLanguage)'==''">en-US</NeutralLanguage>
  </PropertyGroup>

   <!-- Workaround for RC3 VS -->
  <ItemGroup Condition="'$(BuildingInsideVisualStudio)'!='true'">
    <Compile Include="**\*.cs" />
  </ItemGroup>
</Project>